//MULTIPLEXOR DE 4 ENTRADAS

//MODULO//////////////////////////////////////////////////////
module	mux161j	(i,s,x);
	//PUERTOS
	//sentido	tipo	tama単o	nombre
	input					[15:0]	i;
	input					[3:0]		s;
	output							x;
	//ALAMBRES Y REGISTROS
	//tipo				tama単o	nombre
	wire					         x0,x1;

//ASIGNACIONES////////////////////////////////////////////////
	//se単ales
	//assign	se単al=valor/operacion
	
	//componente
	//nombre   etiqueta   (puertos)
	mux81a     u0         (.i(i[7:0]) ,.s(s[3:1]),.x(x0));  
	mux81a     u1         (.i(i[15:8]),.s(s[3:1]),.x(x1));  
	
	mux21a     u2         (.a(x0),.b(x1),.s(s[0]),.x(x));
	//secuenciales
	

endmodule
